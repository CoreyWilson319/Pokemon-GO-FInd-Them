<ul>
    <% moveList.forEach(move => { %>
    <% if (move.pokemon_id == id){  %>
    <li id="details" >Name: <%=move.pokemon_name%></li>
    <li id="details" >Form: <%=move.form%></li>
    <li id="details" >Charge Moves: <%=move.charged_moves%></li>
    <li id="details" >Fast Moves: <%=move.fast_moves%></li>
    <li id="details" >Elite Fast Moves: <%= move.elite_fast_moves %></li>
    <li id="details" >Elite Charged Moves: <%= move.elite_charged_moves %></li>
    <% candyList.forEach(candyTier => { %>
        <% candyTier.forEach(pokemon => { %>
            <% if (pokemon.pokemon_id == id && move.form === pokemon.form) { %>
                <li id="details">Form: <%=pokemon.form%></li>
                <li id="details">Candies to Evolve: <%=pokemon.candy_required%></li>
                <li><br></li>
                <% } %>
                <% }) %>
                <% }) %>
    <li><br></li>
    <% } %>
    <% }); %>
                <li><a id="links" class="statsLink" href="stats/<%=id%>">Stats</a></li>
</ul>
