<h2 id="title">Legendary Pokemon</h2>
<p id="description">Click the number to add the corresponding pokemon to your watchlist</p>
<ul>
    <% pokemonList.forEach(poke => { %>
        <li> <a href="/pokemon/<%=poke.id%>"><%= poke.name %>, Raid Level: <%= poke.raid_level %></a></li>
    <% }); %>
</ul>